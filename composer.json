{
    "name": "okaufmann/laravel-receipt-interpreter",
    "description": "Scan a receipt and get some data out of it.",
    "keywords": [],
    "license": "",
    "type": "library",
    "authors": [
        {
            "name": "Oliver Kaufmann",
            "email": "okaufmann91@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.4.*",
        "illuminate/support": "5.4.*",
        "thiagoalessio/tesseract_ocr": "1.0.0-RC",
        "spatie/image": "^1.1",
        "spatie/regex": "^1.1",
        "federicoginosa/textcleaner": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0",
        "orchestra/testbench": "~3.3.0|~3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Okaufmann\\ReceiptInterpreter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okaufmann\\Tests\\ReceiptInterpreter\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
